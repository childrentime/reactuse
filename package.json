{
  "name": "@reactuses/reactuses-monorepo",
  "packageManager": "pnpm@7.28.0",
  "license": "Unlicense",
  "homepage": "https://www.reactuse.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/childrentime/reactuse"
  },
  "bugs": {
    "url": "https://github.com/childrentime/reactuse/issues"
  },
  "sideEffects": false,
  "scripts": {
    "newHook": "ts-node scripts/newHook.ts",
    "lint": "eslint ./packages/core && eslint ./packages/ts-document",
    "lint:fix": "eslint ./packages/core --fix && eslint ./packages/ts-document --fix",
    "docs:dev": "pnpm --filter website-docusaurus run dev",
    "build": "pnpm --filter @reactuses/core run build",
    "core:dev": "pnpm --filter core run dev & pnpm build:types",
    "size": "tsx scripts/exportSize.mts"
  },
  "dependencies": {
    "bunchee": "^4.4.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime-corejs3": "^7.24.1",
    "@reactuses/core": "workspace:*",
    "@ririd/eslint-config": "0.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.19.29",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "esno": "^0.16.3",
    "export-size": "^0.5.2",
    "fast-glob": "^3.3.2",
    "filesize": "^8.0.7",
    "fs-extra": "^10.1.0",
    "lodash-es": "^4.17.21",
    "markdown-table": "^3.0.3",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "typescript",
        "@types/react"
      ]
    }
  }
}
