{
  "name": "@reactuses/core",
  "version": "2.2.5",
  "license": "Unlicense",
  "homepage": "https://www.reactuse.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/childrentime/reactuse",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/childrentime/reactuse/issues"
  },
  "keywords": [
    "react",
    "react-use",
    "react hooks",
    "hook",
    "hooks",
    "component"
  ],
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint \"{hooks,tests}/**/*.{ts,tsx}\"",
    "build": "esno scripts/build.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c --watch",
    "publish:ci": "esno scripts/publish.ts",
    "release:prepare": "bump",
    "release": "esno scripts/release.ts",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "screenfull": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "babel-jest": "^29.0.2",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.15",
    "esbuild-register": "^3.4.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.10.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "@testing-library/react": "^13.4.0"
  }
}
