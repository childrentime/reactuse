{
  "editor.tabSize": 2,
  "conventionalCommits.scopes": [
    "website",
    "core"
  ],
  "eslint.validate": [
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "json"
  ],
  "eslint.runtime": "node",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  }
}
